{
    "name": "somnambulist/laravel-doctrine-project",
    "description": "Laravel 5.3 with Doctrine, Twig and other Goodies",
    "keywords": ["framework", "laravel", "doctrine", "twig"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7",
        "beberlei/assert": "2.5.*",
        "eloquent/enumeration": "5.1.1",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "laravel-doctrine/orm": "1.2.*",
        "laravel-doctrine/migrations": "1.1.*",
        "laravel-doctrine/acl": "1.0.*",
        "predis/predis": "~1.0",
        "rcrowe/twigbridge": "0.9.*",
        "twig/extensions": "1.3.0",
        "somnambulist/laravel-doctrine-behaviours": "0.4.*",
        "somnambulist/laravel-doctrine-domain-events": "*",
        "somnambulist/laravel-doctrine-entity-audit": "0.4.*",
        "somnambulist/laravel-doctrine-entity-validation": "*",
        "somnambulist/laravel-domain-input-mapper": "*",
        "somnambulist/laravel-environment-loader": "*"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "*",
        "laravel/homestead": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "storage/cache/hydrators"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ],
        "psr-4": {
            "App\\Tests\\": "tests/unit/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\"",
            "php -r \"copy('Homestead.dist.yaml', 'Homestead.yaml');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    }
}
